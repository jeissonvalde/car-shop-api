  "repository": {
    "type": "git",
    "url": "git+https://github.com/jeissonvalde/car-store-api.git"
  },
  "author": "Jeisson A. Valderrama",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/jeissonvalde/car-store-api/issues"
  },
  "eslintConfig": {
    "parserOptions": {
      "project": "./tsconfig.json"
    },
    "extends": [
      "./node_modules/ts-standard/eslinrc.json"
    ]
  },
  "homepage": "https://github.com/jeissonvalde/car-store-api#readme",
  "devDependencies": {
    "@types/express": "^4.17.14",
    "ava": "^5.1.0",
    "nodemon": "^2.0.20",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.4"
  },
  "dependencies": {
    "chalk": "^5.2.0",
    "debug": "^4.3.4",
